<?xml version="1.0" encoding="utf-8"?>
<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <PackageId>Partas.GitNet</PackageId>
        <Title>Partas.GitNet</Title>
        <Company>Partas</Company>
        <Description>Git that .Net F#</Description>
        <PackageLicenseExpression>GPL-3.0-or-later</PackageLicenseExpression>
        <TargetFramework>net8.0</TargetFramework>
        <RepositoryUrl>https://github.com/shayanhabibi/Partas.GitNet/</RepositoryUrl>
        <RepositoryType>git</RepositoryType>
        <PackageTags>Partas;Semver;Git;AutoVersioning;CI;F#;FSharp;Automation;DevOps</PackageTags>
        <Authors>Shayan Habibi</Authors>
        <Copyright>Copyright © Shayan Habibi 2025</Copyright>
        <!--Package & Version are AutoGenerated. Do not modify outside of your GitNet CI, or if you no longer plan on using GitNet.--><Version>2.0.0</Version>
        <!--Package & Version are AutoGenerated. Do not modify outside of your GitNet CI, or if you no longer plan on using GitNet.--><PackageVersion>2.0.0</PackageVersion>
        <GenerateDocumentationFile>true</GenerateDocumentationFile>
        <PackageReadmeFile>README.md</PackageReadmeFile>
        <PackageIcon>Partas_d00b_00a icon.png</PackageIcon>
    </PropertyGroup>

    <ItemGroup>
        <None Include="../README.md" Pack="true" PackagePath="\">
            <Link>Public/README.md</Link>
        </None>
        <None Include="..\public\Partas_d00b_00a icon.png">
            <Pack>True</Pack>
            <PackagePath>\</PackagePath>
            <Link>Public/icon.png</Link>
        </None>
        <Compile Include="Spec.fs" />
        <Compile Include="Types.fs" />
        <Compile Include="Config.fs" />
        <Compile Include="GitHub.fs" />
        <Compile Include="GitNetRuntime.fs" />
        <Compile Include="RepoCracker.fs" />
        <Compile Include="GitHelpers.fs" />
        <Compile Include="GitCollection.fs" />
        <Compile Include="Renderer.fs" />
        <Compile Include="MarkdownWriter.fs" />
        <Compile Include="BuildHelpers.fs" />
        <Compile Include="Program.fs" />
    </ItemGroup>

    <ItemGroup>
      <PackageReference Include="Fake.DotNet.AssemblyInfoFile" Version="6.1.3" />
      <PackageReference Include="Fake.DotNet.Cli" Version="6.1.3" />
      <PackageReference Include="Fake.IO.FileSystem" Version="6.1.3" />
      <PackageReference Include="FSharp.Formatting.Markdown" Version="1.0.0" />
      <PackageReference Include="FsToolkit.ErrorHandling" Version="5.0.1" />
      <PackageReference Include="Partas.LibGit2Sharp.FSharp" Version="0.3.0" />
      <PackageReference Include="Partas.Tools.ConventionalCommits" Version="0.3.0" />
      <PackageReference Include="Partas.Tools.SepochSemver" Version="0.4.1" />
    </ItemGroup>

    <ItemGroup>
      <ProjectReference Include="..\Partas.GitNet.Markdown\Partas.GitNet.Markdown.fsproj" />
    </ItemGroup>

</Project>
